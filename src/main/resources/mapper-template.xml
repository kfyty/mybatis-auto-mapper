<?xml version="1.0" encoding="UTF-8"?>
<mapper>
    <insert id="insert" parameterType="%s">
        insert into %s ( %s ) values ( %s )
    </insert>

    <insert id="insertAll" parameterType="%s">
        <foreach collection="%s" item="%s" open="" close="" separator=";">
            insert into %s values ( %s )
        </foreach>
    </insert>

    <update id="update" parameterType="%s">
        update %s
        &lt;set&gt;
            %s
        &lt;/set&gt;
        where %s = %s
    </update>

    <update id="updateAll" parameterType="%s">
        <foreach collection="%s" item="%s" open="" close="" separator=";">
            update %s
            &lt;set&gt;
                %s
            &lt;/set&gt;
            where %s = %s
        </foreach>
    </update>

    <select id="findBy" resultType="%s">
        select %s from %s where ( %s )
    </select>

    <select id="findAll" resultType="%s">
        select %s from %s
    </select>

    <delete id="deleteBy">
        delete from %s where ( %s )
    </delete>

    <delete id="deleteAll">
        delete from %s
    </delete>
</mapper>
